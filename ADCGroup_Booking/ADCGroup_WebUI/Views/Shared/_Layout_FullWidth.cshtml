
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ADC Booking</title>
    <link rel="icon" type="image/png" href="~/Asset/images/Logo2.png" />
    <link href="~/Asset/css/styleBasicLayout.css" rel="stylesheet" />
    <link href="~/Asset/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Asset/css/styleNpLayout.css" rel="stylesheet" />
    <link href="~/Asset/css/stypeNp-HomeBooking.css" rel="stylesheet" />
</head>
<body>
    <section class="c-menu-slider">
        <input type="checkbox" name="c-menu-btn" id="c-menu-btn">
        <label for="c-menu-btn" class="c-menu-btn">
            <div class="c-menu-btn__indicator">
                <div class="c-menu-btn__indicator--1"></div>
                <div class="c-menu-btn__indicator--2"></div>
                <div class="c-menu-btn__indicator--3"></div>
            </div>
        </label>
        <label for="c-menu-btn" class="c-menu-btn--is-active"></label>
        <div class="c-menu-slider__content">
            <ul class="c-menu-items">
                <li class='item-1'>@Html.ActionLink("Trang Chủ", "Index", "Home")</li>
                <li class='item-2'>@Html.ActionLink("Phòng Họp", "HomeBooking", "Home")</li>
                <li class='item-3'>@Html.ActionLink("Building...", "Index", "Building")</li>
                <li class='item-4'>@Html.ActionLink("Building...", "Index", "Building")</li>
                <li class='item-5'>@Html.ActionLink("Building...", "Index", "Building")</li>
            </ul>
            <a href="/Login/Home" class="c-menu-btn-bottom">
                Đăng Xuất
                <hr class="c-menu-btn-bottom-underline">
            </a>
        </div>
    </section>
    <!--===== END OF RIGHT SLIDER MENU =====-->
    <section class="main">
        @RenderBody()
    </section>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</body>
</html>
